{
  "env": {
    "browser": true,
    "es6": true,
    "node": true
  },
  "extends": [
    "airbnb-base"
  ],
  "globals": {
    "Atomics": "readonly",
    "SharedArrayBuffer": "readonly"
  },
  "parserOptions": {
    "ecmaVersion": 2018,
    "sourceType": "module"
  },
  "rules": {
    "no-console": 0,
    "eol-last": 0,
    "linebreak-style": 0,
    "no-underscore-dangle": 0,
    "no-tabs": 0,
    "operator-linebreak": 0
  }
}